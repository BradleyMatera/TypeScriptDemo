{
  "name": "typescript-card-flip-game",
  "version": "1.0.0",
  "description": "A TypeScript card flip memory game.",
"scripts": {
  "start": "parcel index.html",
  "build": "tsc && parcel build index.html",
  "dev": "tsc --watch",
  "lint": "eslint 'src/**/*.{ts,tsx}'",
  "lint:fix": "eslint 'src/**/*.{ts,tsx}' --fix",
  "sass": "sass src/styles/main.scss dist/main.css --no-source-map --style=compressed",
  "sass:watch": "sass --watch src/styles/main.scss:dist/main.css"
},
  "dependencies": {
    "parcel": "^2.7.0"
  },

  "devDependencies": {
    "@types/node": "^18.0.0",
    "typescript": "^4.9.4",
    "eslint": "^8.0.0",
    "eslint-plugin-import": "^2.29.4",
    "eslint-plugin-node": "^11.1.0",
    "sass": "^1.56.0"
  },
  "eslintConfig": {
    "env": {
      "browser": true,
      "es2021": true
    },
    "extends": [
      "eslint:recommended",
      "plugin:@typescript-eslint/recommended"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
      "ecmaVersion": "latest",
      "sourceType": "module"
    },
    "plugins": [
      "@typescript-eslint"
    ],
    "rules": {
      "@typescript-eslint/no-unused-vars": ["error"]
    }
  },
  "browserlist": [
    ">0.2%",
    "not dead",
    "not op_mini all"
  ],
  "author": "Bradley Matera",
  "license": "MIT"
}